<?xml version="1.0" encoding="utf-8"?><scraper framework="1.1" date="2015-01-17" name="jav" content="movies" language="en">
	<NfoUrl dest="3">
		<RegExp input="$$1" output="\1" dest="3">
			<expression />
		</RegExp>
	</NfoUrl>
    
    <!-- http://movies.yahoo.co.jp/search/?query=%E3%82%B6%E3%83%BB%E3%82%B3%E3%83%BC%E3%83%AB -->
    
	<CreateSearchUrl clearbuffers="no" dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://movies.yahoo.co.jp/search/?query=\1&lt;/url&gt;" dest="3">
			<expression></expression>
		</RegExp>
		
	</CreateSearchUrl>
	
        
    
    <GetSearchResults clearbuffers="no" dest="8">
     
     <!--
      <li class="col" data-cinema-id="24452">
      <a href="/movie/48%E6%99%82%E9%96%93/24452/"  title=" 48時間 (1982)" data-ylk="slk:img;pos:1">
      <div  class="thumbnail__figure " style="background-image:url(http://iwiz-movies.c.yimg.jp/im_sigg45lk.VmEh6N0KsmxrTFurg---x510/c/movies/pict/p/p/73/ea/134837_01.jpg)">
      </div>
      </a>
      
      <li class="col" data-cinema-id="323638">
      <a href="/movie/%E3%81%8B%E3%82%82%E3%82%81%E9%A3%9F%E5%A0%82/323638/"  title=" かもめ食堂 (2005)" data-ylk="slk:img;pos:1">
      <div  class="thumbnail__figure " style="background-image:url(http://iwiz-movies.c.yimg.jp/im_sigg5IWC0M6_SRqYeuaRL4Sw9A---x510/c/movies/pict/p/p/19/f3/144808_01.jpg)">
      </div>
      </a>
      
      
      http://movies.yahoo.co.jp/movie/48%E6%99%82%E9%96%93/24452/
      作品トップ　<a href="/movie/48%E6%99%82%E9%96%93/24452/　$1
      あらすじは　<a href="/movie/48%E6%99%82%E9%96%93/24452/story/"  >　$2
      クレジット　<a href="/movie/48%E6%99%82%E9%96%93/24452/credit/"  > $3
      
      -->
     
     
        <RegExp input="$$6" output="&lt;?xml version=&quot;1.02&quot; encoding=&quot;UTF-8&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="8">
            <RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\4&lt;/title&gt;&lt;url&gt;http://movies.yahoo.co.jp/movie/\2/\1/&lt;/url&gt;&lt;url&gt;http://movies.yahoo.co.jp/movie/\2/\1/story/&lt;/url&gt;&lt;url&gt;http://movies.yahoo.co.jp/movie/\2/\1/credit/&lt;/url&gt;&lt;/entity&gt;" dest="6">
                <expression repeat="yes" noclean="1">&lt;li class="col" data-cinema-id="(.*?)"&gt;.*?&lt;a href=&quot;/movie/(.*?)/([0-9]*?)/".*?title="(.*?)"</expression>
            </RegExp>
        <expression noclean="1"></expression>
        </RegExp>
   </GetSearchResults>
        

        
	<GetDetails dest="4">
		<RegExp input="$$8" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
            <!--    タイトルは　$1の
             <tr>
             <th class="opacity-60">タイトル</th>
             <td>48時間</td>
             </tr>
             -->
			<RegExp input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="8">
				<expression>タイトル&lt;/th&gt;.*?&lt;td&gt;(.*?)&lt;/td&gt;</expression>
			</RegExp>
            <!--
             <th class="opacity-60">製作年度</th>
             <td>1982年</td>
             -->
            <RegExp input="$$1" output="&lt;year&gt;\1&lt;/year&gt;" dest="8+">
				<expression>製作年度&lt;/th&gt;.*?&lt;td&gt;(.*?)年&lt;/td&gt;</expression>
			</RegExp>
            
            
            <!--
             <th class="opacity-60">上映時間</th>
             <td>96分</td>
             -->
            <RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="8+">
                <expression>上映時間&lt;/th&gt;.*?&lt;td&gt;(.*?)分&lt;/td&gt;</expression>
            </RegExp>
            
            
            <!-- 監督は　$1の
             <dl class="dl dl--inline text-small no-space-bottom">
             <dt>監督</dt><dd><a href="/person/%E3%82%A6%E3%82%A9%E3%83%AB%E3%82%BF%E3%83%BC%E3%83%BB%E3%83%92%E3%83%AB/5941/">ウォルター・ヒル</a></dd>
             </dl>

             -->
            <RegExp input="$$2" output="&lt;director&gt;\1&lt;/director&gt;" dest="8+">
                <expression>&lt;dt&gt;監督&lt;/dt&gt;&lt;dd&gt;&lt;a href=&quot;.*&quot;&gt;(.*?)&lt;/a&gt;&lt;/dd&gt;</expression>
            </RegExp>
            
            
            
            
            <!-- サムネイルは$1で
             <p class="thumbnail__figure " style="background-image:url(http://iwiz-movies.c.yimg.jp/im_sigg2.Mc66X1Lt_43tBMvApxiw---x680/c/movies/pict/p/p/73/ea/134837_01.jpg)"></p>

             -->
            
			<RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="8+">
				<expression>&quot;background-image:url\((.*?)\)&quot;&gt;&lt;/p&gt;</expression>
			</RegExp>
            
			
            <!--　キャストは $1で

             <li class="col" itemprop="actor" itemscope itemtype="http://schema.org/Person">
             <a href="/person/%E3%83%8B%E3%83%83%E3%82%AF%E3%83%BB%E3%83%8E%E3%83%AB%E3%83%86%E3%82%A3/38722/" title="ニック・ノルティ" >
             <div class="thumbnail__figure" style="background-image:url(http://iwiz-movies.c.yimg.jp/c/movies/pict/w/p/86/7d/38722.jpg)">
             </div>
             </a>
             <div class="thumbnail__caption">
             <h3 class="text-xsmall" title="ニック・ノルティ">
             <a href="/person/%E3%83%8B%E3%83%83%E3%82%AF%E3%83%BB%E3%83%8E%E3%83%AB%E3%83%86%E3%82%A3/38722/"  itemprop="url"><span itemprop="name">ニック・ノルティ</span></a>
             </h3>
             <p class="text-xsmall">ジャック・ケイツ</p>
             </div>
             </li>

             <a href="/person/%E3%83%A1%E3%83%AB%E3%83%BB%E3%82%AE%E3%83%96%E3%82%BD%E3%83%B3/9381/" title="メル・ギブソン" >
             <div class="thumbnail__figure" style="background-image:url(http://iwiz-movies.c.yimg.jp/c/movies/pict/w/p/ff/cd/9381.jpg)">
             </div>
             </a>
             <div class="thumbnail__caption">
             <h3 class="text-xsmall" title="メル・ギブソン">
             <a href="/person/%E3%83%A1%E3%83%AB%E3%83%BB%E3%82%AE%E3%83%96%E3%82%BD%E3%83%B3/9381/"  itemprop="url"><span itemprop="name">メル・ギブソン</span></a>
             </h3>
      
             -->
            <RegExp input="$$1" output="\1" dest="7">
                <expression noclean="1">キャスト&lt;/h2&gt;(.*?)&lt;div id=&quot;ad-vo&quot;&gt;</expression>
            </RegExp>
            
			<RegExp input="$$7" output="&lt;actor&gt;&lt;name&gt;\2&lt;/name&gt;&lt;thumb&gt;\1&lt;/thumb&gt;&lt;/actor&gt;" dest="8+">
                <expression noclean="1" repeat="yes">&lt;div class=&quot;thumbnail__figure&quot; style=&quot;background-image:url\((.*?)\)&quot;&gt;.*?itemprop=&quot;url&quot;&gt;&lt;span itemprop=&quot;name&quot;&gt;(.*?)&lt;/span&gt;</expression>

                
            </RegExp>
            
            
            
            
            <!-- ジャンル
             <tr>
             <th class="opacity-60">ジャンル</th>
             <td><ul class="list-inline no-space-bottom"><li class="pr1em"><a href="/movie/?genre=action">アクション</a></li><li class="pr1em"><a href="/movie/?genre=comedy">コメディ</a></li></ul></td>
             </tr>
            -->
           
           
            <RegExp input="$$1" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="8+">
                    <expression repeat="yes">/movie/\?genre.*?"&gt;(.*?)&lt;/a&gt;</expression>
			</RegExp>
         
            
            
            <!-- あらすじは　＄２
             <p class="text-readable">
             　野外労働中の囚人が、仲間の助けを得て脱走した。犯人に相棒を殺された刑事ジャックは、彼らとつながりのある、服役中の黒人レジーに捜査協力を求める。48時間の仮釈放という条件で、二人は凶悪犯の追跡に当たるが……。ニック・ノルティ、エディ・マーフィの凸凹コンビによるやりとりが最高のアクション・コメディ。
             <p class="text-xsmall align-right">
          
             -->
            
			<RegExp input="$$2" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="8+">
				<expression trim="1">&lt;p class=&quot;text-readable&quot;&gt;(.*?)&lt;p class</expression>
			</RegExp>
			<expression noclean="1"/>
		</RegExp>
	</GetDetails>
</scraper>
